{
  "name": "vue-ssr-service",
  "version": "0.1.0",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./client": {
      "import": "./dist/client.js",
      "types": "./dist/client.d.ts"
    },
    "./vite": {
      "import": "./dist/vite.js",
      "types": "./dist/vite.d.ts"
    },
    "./render": {
      "import": "./dist/render.js",
      "types": "./dist/render.d.ts"
    },
    "./server": {
      "import": "./dist/server.js",
      "types": "./dist/server.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "bin": "./dist/cli.js",
  "type": "module",
  "scripts": {
    "start": "bun run src/cli.ts",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "build": "tsup",
    "prepack": "tsup"
  },
  "dependencies": {
    "@hono/node-server": "^1.14.0",
    "@hono/zod-validator": "^0.4.3",
    "commander": "^13.1.0",
    "hono": "^4.7.5",
    "pino": "^9.6.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/bun": "^1.2.13",
    "@vitest/coverage-v8": "3.0.9",
    "happy-dom": "^17.4.7",
    "prettier": "^3.5.3",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "vite": "^6.2.3",
    "vitest": "^3.0.9"
  },
  "peerDependencies": {
    "vue": "^3"
  },
  "engines": {
    "bun": ">=1.2.4",
    "node": ">=22.0.0"
  },
  "tsup": {
    "entry": [
      "src/index.ts",
      "src/client.ts",
      "src/vite.ts",
      "src/cli.ts",
      "src/server.ts",
      "src/render.ts"
    ],
    "format": [
      "esm"
    ],
    "dts": true,
    "sourcemap": true,
    "clean": true
  }
}